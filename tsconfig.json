{
    "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "ESNext",
      "target": "es5",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "strictNullChecks": true,
      "paths": {
        "Shared/*": ["src/shared/*"],
        "Components/*": ["src/components/*"],
        "src/*": ["src/*"],
      }
    }
  }